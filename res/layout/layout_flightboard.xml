<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layoutflightboard_container_linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundDarkGray"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/layoutflightboard_flightdynamics_topline_imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:src="@drawable/layoutflightboard_flightdynamicsline_arrive" />

	<LinearLayout
	    android:id="@+id/layoutflightboard_flightdynamics_search_linearLayout"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:paddingBottom="5dp"
	    android:paddingTop="5dp" >

		<LinearLayout
		    android:layout_width="150dp"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >
        </LinearLayout>

	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_weight="1"
	        android:orientation="vertical" >

	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="match_parent"
	            android:layout_gravity="center_horizontal"
	            android:orientation="horizontal" >
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:text="航班动态"
		            android:textColor="@color/white"
		            android:textSize="20sp"
		            android:typeface="serif" />
	        </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="150dp"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >
	        <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:layout_weight="1"
		        android:orientation="vertical" >

		        <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="right"
		            android:layout_marginRight="10dp"
		            android:src="@drawable/searchinco_white" />

		    </LinearLayout>
	    </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/flightDynamicFlightInfoBg"
        android:layout_marginTop="5dp"
        android:orientation="horizontal" >

		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >

	        <TextView
	            android:id="@+id/layoutflightboard_flightcompanyNumTextView"
	            android:layout_width="wrap_content"
	            android:layout_height="25dp"
	            android:layout_gravity="right"
	            android:gravity="bottom"
	            android:text="深圳航空ZH1563"
	            android:textColor="@color/white"
	            android:textSize="15sp"
	            android:typeface="serif" />
	        
	        <TextView
	            android:id="@+id/layoutflightboard_fromCity_textView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="right"
	            android:text="上海"
	            android:textColor="@color/white"
	            android:textSize="23sp"
	            android:typeface="serif" />

	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="right"
	            android:layout_marginTop="3dp"
	            android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/layoutflightboard_fromCityWeather_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="right"
		            android:text="-3~-6"
		            android:textColor="@color/weatherColor"
		            android:textSize="12sp"
		            android:typeface="serif" />

		        <ImageView
		            android:id="@+id/layoutflightboard_fromCityWeather_imageView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="3dp"
		            android:src="@drawable/weather_cloud" />

		    </LinearLayout>

		    <TextView
		        android:id="@+id/layoutflightboard_fromTerminal_textView"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_gravity="right"
		        android:layout_marginTop="3dp"
		        android:text="首都机场T3"
		        android:textColor="@color/white"
		        android:textSize="16sp"
		        android:typeface="serif" />

		    <LinearLayout
		        android:layout_width="80dp"
		        android:layout_height="2dp"
		        android:layout_gravity="right"
		        android:layout_marginTop="3dp"
		        android:background="@color/boardDividerBlue"
		        android:orientation="horizontal" >

		    </LinearLayout>

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_gravity="right"
		        android:layout_marginTop="3dp"
		        android:orientation="horizontal" >

		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="right"
		            android:text="计划起飞"
		            android:textColor="@color/weatherColor"
		            android:textSize="15sp"
		            android:typeface="serif" />

		        <LinearLayout
		            android:layout_width="45dp"
		            android:layout_height="wrap_content"
		            android:orientation="vertical" >
			        <TextView
			            android:id="@+id/layoutflightboard_planFromTime_textView"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:layout_gravity="right"
			            android:text="14:55"
			            android:textColor="@color/white"
			            android:textSize="15sp"
			            android:typeface="serif" />
		        </LinearLayout>
		    </LinearLayout>

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_gravity="right"
		        android:layout_marginTop="3dp"
		        android:orientation="horizontal" >

		         <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="right"
		            android:text="实际起飞"
		            android:textColor="@color/weatherColor"
		            android:textSize="15sp"
		            android:typeface="serif" />
		         <LinearLayout
		            android:layout_width="45dp"
		            android:layout_height="wrap_content"
		            android:orientation="vertical" >
			        <TextView
			            android:id="@+id/layoutflightboard_actualFromTime_textView"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:layout_gravity="right"
			            android:text="-- : --"
			            android:textColor="@color/white"
			            android:textSize="15sp"
			            android:typeface="serif" />
		        </LinearLayout>
		    </LinearLayout>
	    </LinearLayout>

		<LinearLayout
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_gravity="center_vertical"
		    android:layout_marginLeft="10dp"
		    android:layout_marginRight="10dp"
		    android:layout_marginTop="25dp"
		    android:orientation="vertical" >

			<TextView
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:text="准点率"
			    android:textColor="@color/boardDividerBlue"
			    android:textSize="12sp"
			    android:typeface="serif" />

			<TextView
			    android:id="@+id/layoutflightboard_ontimeRate_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:text="90%"
			    android:textColor="@color/boardDividerBlue"
			    android:textSize="12sp"
			    android:typeface="serif" />

	    </LinearLayout>
		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >

			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="25dp"
			    android:layout_gravity="bottom"
			    android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/layoutflightboard_date_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="bottom"
		            android:text="2013-12-14"
		            android:textColor="@color/textBlue"
		            android:textSize="12sp"
		            android:typeface="serif" />

		        <LinearLayout
		            android:id="@+id/layoutflightboard_refresh_linearLayout"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:layout_gravity="bottom"
		            android:orientation="vertical" >

			        <ImageView
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:layout_gravity="right"
			            android:layout_marginRight="10dp"
			            android:src="@drawable/layoutflightboard_refresh" />

			    </LinearLayout>
		    </LinearLayout>

			<TextView
			    android:id="@+id/layoutflightboard_toCity_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:text="北京"
			    android:textColor="@color/white"
			    android:textSize="23sp"
			    android:typeface="serif" />

	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="3dp"
	            android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/layoutflightboard_toCityWeather_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="-3~-6"
		            android:textColor="@color/weatherColor"
		            android:textSize="12sp"
		            android:typeface="serif" />

		        <ImageView
		            android:id="@+id/layoutflightboard_toCityWeather_imageView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="3dp"
		            android:src="@drawable/weather_cloud" />

		    </LinearLayout>

		    <TextView
		        android:id="@+id/layoutflightboard_toTerminal_textView"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="3dp"
		        android:text="首都机场T3"
		        android:textColor="@color/white"
		        android:textSize="16sp"
		        android:typeface="serif" />

		    <LinearLayout
		        android:layout_width="80dp"
		        android:layout_height="2dp"
		        android:layout_marginTop="3dp"
		        android:background="@color/boardDividerBlue"
		        android:orientation="horizontal" >

		    </LinearLayout>

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="3dp"
		        android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/layoutflightboard_planToTime_textView"
		            android:layout_width="45dp"
		            android:layout_height="wrap_content"
		            android:text="14:55"
		            android:textColor="@color/white"
		            android:textSize="15sp"
		            android:typeface="serif" />
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="计划降落"
		            android:textColor="@color/weatherColor"
		            android:textSize="15sp"
		            android:typeface="serif" />
		    </LinearLayout>

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="3dp"
		        android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/layoutflightboard_actualToTime_textView"
		            android:layout_width="45dp"
		            android:layout_height="wrap_content"
		            android:text="-- : --"
		            android:textColor="@color/white"
		            android:textSize="15sp"
		            android:typeface="serif" />
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="实际降落"
		            android:textColor="@color/weatherColor"
		            android:textSize="15sp"
		            android:typeface="serif" />
		    </LinearLayout>
	    </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
		<ImageView
         android:id="@+id/layoutflightboard_flightStatus_imageView"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:src="@drawable/layoutflightboard_status_arrive" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:orientation="vertical" >

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="10dp"
		    android:layout_marginLeft="15dp"
		    android:layout_marginRight="15dp"
		    android:layout_marginTop="10dp"
		    android:orientation="horizontal" >

			<TextView
	            android:id="@+id/layoutflightboard_fromAirportInfo_textView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:text="首都机场信息"
	            android:textColor="@color/white"
	            android:textSize="20sp"
	            android:typeface="serif" />
			<LinearLayout
			    android:id="@+id/layoutflightboard_fromAirportInfo_linearLayout"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:orientation="vertical">
		        <ImageView
			         android:layout_width="wrap_content"
			         android:layout_height="wrap_content"
			         android:layout_gravity="right"
			         android:src="@drawable/arrow_right_orange_medium" />
		    </LinearLayout>
	    </LinearLayout>
		<LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="1dp"
		    android:background="@color/flightDynamicPlanInfoDivider"
	        android:orientation="horizontal"/>

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="10dp"
		    android:layout_marginLeft="15dp"
		    android:layout_marginRight="15dp"
		    android:layout_marginTop="10dp"
		    android:orientation="horizontal" >

			<TextView
	            android:id="@+id/layoutflightboard_toAirportInfo_textView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:text="萧山机场信息"
	            android:textColor="@color/white"
	            android:textSize="20sp"
	            android:typeface="serif" />
			<LinearLayout
			    android:id="@+id/layoutflightboard_toAirportInfo_linearLayout"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:orientation="vertical">
		        <ImageView
			         android:layout_width="wrap_content"
			         android:layout_height="wrap_content"
			         android:layout_gravity="right"
			         android:src="@drawable/arrow_right_orange_medium" />
		    </LinearLayout>
	    </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layoutflightboard_follow_linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="15dp"
        android:background="@color/priceGray"
        android:orientation="vertical"
        android:paddingBottom="10dp"
        android:paddingTop="10dp" >

		<TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="取消关注"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:typeface="serif" />
    </LinearLayout>
    
    <ImageView
        android:id="@+id/layoutflightboard_pageIndicator_imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"
        android:src="@drawable/threepages_first" />
    
    <LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="horizontal" >
	    <LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_gravity="bottom"
		    android:orientation="vertical" >
		    <LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="right"
			    android:orientation="vertical" >
			    <ImageView
				    android:id="@+id/main_page_airserver_imageView"
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:layout_marginRight="20dp"
				    android:src="@drawable/toppage_gotomainpage" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>