<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:ignore="MergeRootFrame" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:orientation="horizontal" >

		<LinearLayout
		    android:id="@+id/return_logo_linearLayout"
		    android:layout_width="150dp"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >

	        <ImageView
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:src="@drawable/arrow_left_returntoprepage" />
        </LinearLayout>

	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_weight="1"
	        android:orientation="horizontal" >

	        <LinearLayout
		        android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:orientation="vertical">

		        <TextView
		            android:id="@+id/flightboard_planeInfo_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_horizontal"
		            android:text="深圳航空ZH1563"
		            android:textColor="@color/textOrange"
		            android:textSize="19sp"
		            android:typeface="serif" />

		        <TextView
		            android:id="@+id/flightboard_date_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_horizontal"
		            android:text="2013-12-14"
		            android:textColor="@color/textOrange"
		            android:textSize="13sp"
		            android:typeface="serif" />
	        </LinearLayout>
		</LinearLayout>

		<LinearLayout
		    android:layout_width="150dp"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >

	    </LinearLayout>
    </LinearLayout>
    <LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="2dp"
	    android:background="@color/panelOrange"
	    android:orientation="horizontal">
	</LinearLayout>
	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="5dp"
        android:orientation="horizontal" >

		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >

	        <TextView
	            android:id="@+id/flightboard_fromCity_textView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="right"
	            android:text="上海"
	            android:textColor="@color/black"
	            android:textSize="25sp"
	            android:typeface="serif" />
	    </LinearLayout>
	    <LinearLayout
		    android:layout_width="50dp"
		    android:layout_height="wrap_content"
		    android:layout_gravity="center_vertical"
		    android:orientation="vertical" >
	    </LinearLayout>
		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >

			<TextView
			    android:id="@+id/flightboard_toCity_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:text="北京"
			    android:textColor="@color/black"
			    android:textSize="25sp"
			    android:typeface="serif" />
	    </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="0dp"
        android:orientation="horizontal" >

		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="right"
	            android:layout_marginTop="3dp"
	            android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/flightboard_fromCityWeather_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="right"
		            android:text="-3~-6"
		            android:textColor="@color/black"
		            android:textSize="12sp"
		            android:typeface="serif" />

		        <ImageView
		            android:id="@+id/flightboard_fromCityWeather_imageView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="3dp"
		            android:src="@drawable/weather_cloud" />

		    </LinearLayout>
	    </LinearLayout>
	    <LinearLayout
		    android:layout_width="50dp"
		    android:layout_height="wrap_content"
		    android:layout_gravity="center_vertical"
		    android:orientation="vertical" >
	    </LinearLayout>
		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >
			<LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginTop="3dp"
	            android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/flightboard_toCityWeather_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="-3~-6"
		            android:textColor="@color/black"
		            android:textSize="12sp"
		            android:typeface="serif" />

		        <ImageView
		            android:id="@+id/flightboard_toCityWeather_imageView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="3dp"
		            android:src="@drawable/weather_cloud" />

		    </LinearLayout>
	    </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="0dp"
        android:orientation="horizontal" >

		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >

	        <TextView
		        android:id="@+id/flightboard_fromTerminal_textView"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_gravity="right"
		        android:layout_marginTop="3dp"
		        android:text="首都机场T3"
		        android:textColor="@color/black"
		        android:textSize="16sp"
		        android:typeface="serif" />
	    </LinearLayout>
	    <LinearLayout
		    android:layout_width="50dp"
		    android:layout_height="wrap_content"
		    android:layout_gravity="center_vertical"
		    android:orientation="vertical" >
	    </LinearLayout>
		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >

			<TextView
		        android:id="@+id/flightboard_toTerminal_textView"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="3dp"
		        android:text="首都机场T3"
		        android:textColor="@color/black"
		        android:textSize="16sp"
		        android:typeface="serif" />
	    </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="5dp"
        android:orientation="horizontal" >

		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >

		    <LinearLayout
		        android:id="@+id/flightboard_fromDivider_linearLayout"
		        android:layout_width="80dp"
		        android:layout_height="2dp"
		        android:layout_gravity="right"
		        android:layout_marginTop="3dp"
		        android:background="@color/boardDividerBlue"
		        android:orientation="horizontal" >
		    </LinearLayout>

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_gravity="right"
		        android:layout_marginTop="3dp"
		        android:orientation="horizontal" >

		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="right"
		            android:text="计划起飞"
		            android:textColor="@color/boardTextGray"
		            android:textSize="17sp"
		            android:typeface="serif" />

		        <LinearLayout
		            android:layout_width="45dp"
		            android:layout_height="wrap_content"
		            android:orientation="vertical" >
			        <TextView
			            android:id="@+id/flightboard_planFromTime_textView"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:layout_gravity="right"
			            android:text="14:55"
			            android:textColor="@color/black"
			            android:textSize="16sp"
			            android:typeface="serif" />
		        </LinearLayout>
		    </LinearLayout>

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_gravity="right"
		        android:layout_marginTop="3dp"
		        android:orientation="horizontal" >

		         <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="right"
		            android:text="实际起飞"
		            android:textColor="@color/boardTextGray"
		            android:textSize="17sp"
		            android:typeface="serif" />
		         <LinearLayout
		            android:layout_width="45dp"
		            android:layout_height="wrap_content"
		            android:orientation="vertical" >
			        <TextView
			            android:id="@+id/flightboard_actualFromTime_textView"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:layout_gravity="right"
			            android:text="-- : --"
			            android:textColor="@color/black"
			            android:textSize="16sp"
			            android:typeface="serif" />
		        </LinearLayout>
		    </LinearLayout>
	    </LinearLayout>

		<LinearLayout
		    android:layout_width="50dp"
		    android:layout_height="wrap_content"
		    android:layout_gravity="top"
		    android:orientation="vertical" >

			<TextView
			    android:id="@+id/flightboard_ontimeRateText_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:text="准点率"
			    android:textColor="@color/boardDividerBlue"
			    android:textSize="12sp"
			    android:typeface="serif" />

			<TextView
			    android:id="@+id/flightboard_ontimeRate_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_horizontal"
			    android:text="90%"
			    android:textColor="@color/boardDividerBlue"
			    android:textSize="12sp"
			    android:typeface="serif" />

	    </LinearLayout>
		<LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:orientation="vertical" >

		    <LinearLayout
		        android:id="@+id/flightboard_toDivider_linearLayout"
		        android:layout_width="80dp"
		        android:layout_height="2dp"
		        android:layout_marginTop="3dp"
		        android:background="@color/boardDividerBlue"
		        android:orientation="horizontal" >
		    </LinearLayout>

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="3dp"
		        android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/flightboard_planToTime_textView"
		            android:layout_width="45dp"
		            android:layout_height="wrap_content"
		            android:text="14:55"
		            android:textColor="@color/black"
		            android:textSize="16sp"
		            android:typeface="serif" />
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="计划降落"
		            android:textColor="@color/boardTextGray"
		            android:textSize="17sp"
		            android:typeface="serif" />
		    </LinearLayout>

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="3dp"
		        android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/flightboard_actualToTime_textView"
		            android:layout_width="45dp"
		            android:layout_height="wrap_content"
		            android:text="-- : --"
		            android:textColor="@color/black"
		            android:textSize="16sp"
		            android:typeface="serif" />
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="实际降落"
		            android:textColor="@color/boardTextGray"
		            android:textSize="17sp"
		            android:typeface="serif" />
		    </LinearLayout>
	    </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
		<ImageView
         android:id="@+id/flightboard_flightStatus_imageView"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:src="@drawable/flightboard_arrive" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="10dp"
        android:background="@color/boardPanelBg"
        android:orientation="vertical" >

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="10dp"
		    android:layout_marginLeft="15dp"
		    android:layout_marginRight="15dp"
		    android:layout_marginTop="10dp"
		    android:orientation="horizontal" >

			<TextView
	            android:id="@+id/flightboard_fromAirportInfo_textView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:text="首都机场信息"
	            android:textColor="@color/callCustomServiceBgGray"
	            android:textSize="20sp"
	            android:typeface="serif" />
			<LinearLayout
			    android:id="@+id/flightboard_fromAirportInfo_linearLayout"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:orientation="vertical">
		        <ImageView
			         android:layout_width="wrap_content"
			         android:layout_height="wrap_content"
			         android:layout_gravity="right"
			         android:src="@drawable/arrow_right_orange_medium" />
		    </LinearLayout>
	    </LinearLayout>
		<LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="1dp"
		    android:background="@color/voicesearchTitleColor"
	        android:orientation="horizontal"/>

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="10dp"
		    android:layout_marginLeft="15dp"
		    android:layout_marginRight="15dp"
		    android:layout_marginTop="10dp"
		    android:orientation="horizontal" >

			<TextView
	            android:id="@+id/flightboard_toAirportInfo_textView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:text="萧山机场信息"
	            android:textColor="@color/callCustomServiceBgGray"
	            android:textSize="20sp"
	            android:typeface="serif" />
			<LinearLayout
			    android:id="@+id/flightboard_toAirportInfo_linearLayout"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:orientation="vertical">
		        <ImageView
			         android:layout_width="wrap_content"
			         android:layout_height="wrap_content"
			         android:layout_gravity="right"
			         android:src="@drawable/arrow_right_orange_medium" />
		    </LinearLayout>
	    </LinearLayout>
    </LinearLayout>

	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="horizontal" >

	    <LinearLayout
	        android:id="@+id/flightboard_follow_linearLayout"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_gravity="bottom"
	        android:layout_marginBottom="20dp"
	        android:layout_marginLeft="15dp"
	        android:layout_marginRight="15dp"
	        android:layout_marginTop="20dp"
	        android:background="@color/textBlue"
	        android:orientation="vertical"
	        android:paddingBottom="10dp"
	        android:paddingTop="10dp" >

			<TextView
		    	android:id="@+id/flightboard_follow_textView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_horizontal"
	            android:text="关注航班"
	            android:textColor="@color/white"
	            android:textSize="20sp"
	            android:typeface="serif" />
	    </LinearLayout>
    </LinearLayout>
</LinearLayout>

