<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:ignore="MergeRootFrame" >

	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:layout_marginLeft="15dp"
	    android:layout_marginRight="15dp"
	    android:orientation="horizontal" >

		<LinearLayout
		    android:id="@+id/return_logo_linearLayout"
		    android:layout_width="150dp"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >

	        <ImageView
	            android:id="@+id/return_logo_imageView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:src="@drawable/arrow_left_returntoprepage" />
        </LinearLayout>

	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_weight="1"
	        android:orientation="vertical" >

	        <LinearLayout
		        android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal">
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:text="机票预订"
		            android:textColor="@color/textOrange"
		            android:textColorHint="@color/white"
		            android:textSize="20dp"
		            android:typeface="serif" />
	        </LinearLayout>
		</LinearLayout>

		<LinearLayout
		    android:layout_width="150dp"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >

	    </LinearLayout>
    </LinearLayout>

	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="45dp"
	    android:background="@color/panelOrange"
	    android:orientation="vertical" >
		
	    <LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:layout_gravity="center_horizontal"
	        android:orientation="horizontal" >

			<TextView
			    android:id="@+id/search_result_title_from_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:layout_marginRight="5dp"
			    android:text="东京 "
			    android:textColor="@color/white"
			    android:textSize="22sp"
			    android:typeface="serif" />
			
			<TextView
			    android:id="@+id/search_result_title_from_code_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:layout_marginRight="10dp"
			    android:text="TYO"
			    android:textColor="@color/white"
			    android:textSize="14sp"
			    android:typeface="serif" />
			
			<ImageView
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:src="@drawable/cityline" />
			
			<TextView
			    android:id="@+id/search_result_title_to_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:layout_marginLeft="10dp"
			    android:text="上海"
			    android:textColor="@color/white"
			    android:textSize="22sp"
			    android:typeface="serif" />
			
			<TextView
			    android:id="@+id/search_result_title_tocode_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:layout_marginLeft="5dp"
			    android:text="SHA"
			    android:textColor="@color/white"
			    android:textSize="14sp"
			    android:typeface="serif" />

		</LinearLayout>
	</LinearLayout>
	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="40dp"
	    android:orientation="horizontal" >

	    <LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:layout_weight="1"
	        android:orientation="vertical" >
			<LinearLayout
	        android:id="@+id/search_result_lastday_linearLayout"
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:layout_gravity="left"
	        android:layout_marginLeft="13dp"
	        android:layout_weight="1"
	        android:orientation="vertical" >
			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:orientation="horizontal" >

		        <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:src="@drawable/arrow_left_gray_small" />

		        <TextView
		            android:id="@+id/search_result_lastday_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="5dp"
		            android:text="前一天"
		            android:textColor="@color/dayIndicatorGray"
		            android:textSize="14sp"
		            android:typeface="serif" />

			</LinearLayout>
			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:orientation="vertical" >

		        <TextView
		            android:id="@+id/search_result_lastday_price_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_horizontal"
		            android:text="￥500"
		            android:textColor="@color/priceGray"
		            android:textSize="12sp"
		            android:typeface="serif" />
			</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="vertical" >
			<LinearLayout
		    android:id="@+id/search_result_today_linearLayout"
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:layout_gravity="center_horizontal"
		    android:orientation="vertical" >
			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/search_result_today_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginRight="5dp"
		            android:text="2014-01-07"
		            android:textColor="@color/textOrange"
		            android:textSize="14sp"
		            android:typeface="serif" />
		        <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:src="@drawable/arrow_down_orange_small" />

			</LinearLayout>
			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:orientation="vertical" >

		        <TextView
		            android:id="@+id/search_result_today_price_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_horizontal"
		            android:text="￥670"
		            android:textColor="@color/panelOrange"
		            android:textSize="12sp"
		            android:typeface="serif" />
			</LinearLayout>
			</LinearLayout>
		</LinearLayout>

		<LinearLayout
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="vertical" >

			<LinearLayout
			    android:id="@+id/search_result_nextday_linearLayout"
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="right"
			    android:layout_marginRight="13dp"
			    android:orientation="vertical" >

			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:orientation="horizontal" >

		        <TextView
		            android:id="@+id/search_result_nextady_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginRight="5dp"
		            android:text="后一天"
		            android:textColor="@color/dayIndicatorGray"
		            android:textSize="14sp"
		            android:typeface="serif" />
		        <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:src="@drawable/arrow_right_gray_small" />

			</LinearLayout>
			<LinearLayout
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:orientation="vertical" >

		        <TextView
		            android:id="@+id/search_result_nextday_price_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_horizontal"
		            android:text="￥580"
		            android:textColor="@color/priceGray"
		            android:textSize="12sp"
		            android:typeface="serif" />

			</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>

	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="1dp"
	    android:background="@color/dividerGray2"
	    android:orientation="vertical" >
	</LinearLayout>
	

    <LinearLayout
        android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
        android:layout_weight="11"  >

		<TextView
		    android:id="@+id/international_ticket_search_result_noResultTextView"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_gravity="center_horizontal"
		    android:layout_marginTop="15dp"
		    android:text="没有找到符合条件的结果"
		    android:textColor="@color/priceGray"
		    android:textSize="17sp"
		    android:typeface="serif" />

        <ExpandableListView
            android:id="@+id/international_ticket_search_result_expandableListView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:cacheColorHint="@android:color/white"
            android:childDivider="@color/classInfoPanelBgColor"
            android:divider="@color/listviewDividerColor"
            android:dividerHeight="2px"
            android:footerDividersEnabled="true"
            android:groupIndicator="@null"
            android:scrollbars="none" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/panelDividerGray"
        android:orientation="vertical" >

	</LinearLayout>

	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="40dp"
	    android:orientation="horizontal" >

		<LinearLayout
		    android:id="@+id/search_result_filter_linearLayout"
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="vertical" >

			<LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal" >

				<TextView
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:layout_gravity="center_vertical"
				    android:text="筛选"
				    android:textColor="@color/filterPriceFromtimeColor"
				    android:textSize="17sp"
				    android:typeface="serif" />

		        <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:src="@drawable/filter_button_gray" />

			</LinearLayout>
		</LinearLayout>
	    <LinearLayout
	        android:id="@+id/search_result_startflying_linearLayout"
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="vertical" >
			<LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal" >

		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:text="起飞"
		            android:textColor="@color/textOrange"
		            android:textSize="17sp"
		            android:typeface="serif" />

		        <ImageView
		            android:id="@+id/leatime_order_imageview"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="3dp"
		            android:src="@drawable/triangle_up_orange" />

			</LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:id="@+id/search_result_price_linearLayout"
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="vertical" >
			<LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal" >

		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:text="价格"
		            android:textColor="@color/filterPriceFromtimeColor"
		            android:textSize="17sp"
		            android:typeface="serif" />

		        <ImageView
		            android:id="@+id/price_order_imageview"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="3dp"
		            android:src="@drawable/triangle_up_gray" />

			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>

