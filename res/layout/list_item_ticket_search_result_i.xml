<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="13dp"
	    android:layout_marginRight="13dp"
	    android:orientation="horizontal" >
		<LinearLayout
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="9"
		    android:orientation="vertical" >
	
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginBottom="5dp"
		        android:layout_weight="1"
		        android:orientation="horizontal" >
		
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_weight="1"
		            android:orientation="horizontal">
		
		            <TextView
		                android:id="@+id/ticket_search_i_flightstarttime_textView"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|left"
		                android:layout_marginRight="5dp"
		                android:text="06:35 -"
		                android:textColor="@color/black"
		                android:textSize="20dp"
		                android:textStyle="bold"
		                android:typeface="serif" />
		
		            <TextView
		                android:id="@+id/ticket_search_i_flightendtime_textView"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|left"
		                android:text="08:35"
		                android:textColor="@color/black"
		                android:layout_marginRight="5dp"
		                android:textSize="15dp"
		                android:typeface="serif" />
		            
		            <TextView
		                android:id="@+id/ticket_search_i_durationDay"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|left"
		                android:text="+1天"
		                android:textColor="@color/priceGray"
		                android:textSize="13dp"
		                android:textStyle="bold"
		                android:typeface="serif" />
		
		        </LinearLayout>
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_weight="1"
		            android:orientation="vertical">
		            <LinearLayout
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|right"
		                android:orientation="horizontal">
		                <TextView
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:layout_gravity="bottom|right"
		                    android:text="￥"
		                    android:textColor="@color/panelOrange"
		                    android:textSize="15dp"
		                    android:typeface="serif" />
		    
		                <TextView
		                    android:id="@+id/ticket_search_i_flightprice_textView"
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:layout_gravity="bottom|right"
		                    android:text="2570"
		                    android:textColor="@color/panelOrange"
		                    android:textSize="20dp"
		                    android:typeface="serif" />
		            </LinearLayout>
		        </LinearLayout>
		    </LinearLayout>
	
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginBottom="3dp"
		        android:layout_weight="1"
		        android:orientation="horizontal" >
	
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_weight="1"
		            android:orientation="horizontal">
		
		            <TextView
		                android:id="@+id/ticket_search_i_flightflyingtime_textView"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|left"
		                android:text="约15小时10分钟"
		                android:textColor="@color/searchResultTinyText"
		                android:textSize="13dp"
		                android:textStyle="bold"
		                android:typeface="serif" />

		        </LinearLayout>
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_weight="1"
		            android:orientation="vertical">
		            <LinearLayout
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|right"
		                android:orientation="horizontal">

		                <TextView
		                    android:id="@+id/ticket_search_i_flightticketleft_textView"
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:layout_gravity="bottom|right"
		                    android:text=">9"
		                    android:textColor="@color/ticketLeftColor"
		                    android:textSize="13dp"
		                    android:textStyle="bold"
		                    android:typeface="serif" />

		                <TextView
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:layout_gravity="bottom|right"
		                    android:text="张"
		                    android:textColor="@color/black"
		                    android:textSize="13dp"
		                    android:textStyle="bold"
		                    android:typeface="serif" />

		            </LinearLayout>
		        </LinearLayout>
		    </LinearLayout>
	
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginBottom="3dp"
		        android:layout_weight="1"
		        android:orientation="horizontal" >
	
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_weight="1"
		            android:orientation="horizontal">

		            <TextView
		                android:id="@+id/ticket_search_i_airlinecompany_textView"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|left"
		                android:text="大韩航空"
		                android:textColor="@color/searchResultTinyText"
		                android:textSize="13dp"
		                android:textStyle="bold"
		                android:typeface="serif" />

		            <TextView
		                android:id="@+id/ticket_search_i_airlinecode_textView"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|left"
		                android:text="KE714"
		                android:textColor="@color/searchResultTinyText"
		                android:textSize="13dp"
		                android:textStyle="bold"
		                android:typeface="serif" />

		        </LinearLayout>
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_weight="1"
		            android:orientation="vertical">
		            <LinearLayout
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|right"
		                android:orientation="horizontal">

		                <TextView
		                    android:id="@+id/ticket_search_i_flightclass_textView"
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:layout_gravity="bottom|right"
		                    android:text="经济舱"
		                    android:textColor="@color/searchResultTinyText"
		                    android:textSize="13dp"
		                    android:textStyle="bold"
		                    android:typeface="serif" />

		            </LinearLayout>
		        </LinearLayout>
		    </LinearLayout>
		
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginBottom="3dp"
		        android:layout_weight="1"
		        android:orientation="horizontal" >
		
		        <ImageView
		            android:id="@+id/ticket_search_i_need_transit_imageView"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_horizontal"
		            android:src="@drawable/line1_2x" />
		    </LinearLayout>
	
		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginBottom="3dp"
		        android:layout_weight="1"
		        android:orientation="horizontal" >
	
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_weight="1"
		            android:orientation="horizontal">

		            <TextView
		                android:id="@+id/ticket_search_i_flightstartairport_textView"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|left"
		                android:text="成田机场"
		                android:textColor="@color/searchResultTinyText"
		                android:textSize="13dp"
		                android:textStyle="bold"
		                android:typeface="serif" />

		        </LinearLayout>
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_weight="1"
		            android:orientation="vertical">
		            <LinearLayout
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_gravity="bottom|right"
		                android:orientation="horizontal">

		                <TextView
		                    android:id="@+id/ticket_search_i_flightstopairport_textView"
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:layout_gravity="bottom|right"
		                    android:text="浦东机场"
		                    android:textColor="@color/searchResultTinyText"
		                    android:textSize="13dp"
		                    android:textStyle="bold"
		                    android:typeface="serif" />

		            </LinearLayout>
		        </LinearLayout>
		    </LinearLayout>
		
		    <LinearLayout
		        android:id="@+id/ticket_search_i_moreflightclassinfo_linearLayout"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginBottom="3dp"
		        android:layout_weight="1"
		        android:orientation="vertical" >
		
		        <ImageView
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_horizontal"
		            android:src="@drawable/datechoice_2x" />
		    </LinearLayout>
	
		    
		</LinearLayout>
	
	
		<LinearLayout
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >
	
			<ImageView
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:layout_marginLeft="5dp"
			    android:src="@drawable/todetailpage_2x" />
	
		</LinearLayout>
		
	</LinearLayout>

	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="2dp"
	    android:background="@color/listviewDividerColor"
	    android:orientation="horizontal" >

    </LinearLayout>
</LinearLayout>
