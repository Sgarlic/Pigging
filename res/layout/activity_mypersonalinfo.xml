<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:ignore="MergeRootFrame" >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingRight="15dp" >
		<LinearLayout
		    android:id="@+id/return_logo_linearLayout"
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >
	        <ImageView
	            android:id="@+id/return_logo_imageView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:src="@drawable/arrow_left_returntoprepage" />
        </LinearLayout>
	    <LinearLayout
	        android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="vertical">
	        <LinearLayout
		        android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal">
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:text="我的波丁"
		            android:textColor="@color/textOrange"
		            android:textSize="20sp"
		            android:typeface="serif" />
	        </LinearLayout>
		</LinearLayout>
		<LinearLayout
	        android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal">
	    </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/panelOrange"
        android:orientation="horizontal" >
	</LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="20dp" >
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="15dp"
		    android:orientation="horizontal"
		    android:paddingBottom="5dp"
		    android:paddingTop="5dp" >

			<LinearLayout
			    android:layout_width="40dp"
			    android:layout_height="wrap_content"
			    android:orientation="horizontal" >

		        <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:src="@drawable/account_gray" />
			</LinearLayout>

		    <TextView
		        android:id="@+id/mypersonalinfo_userName_textView"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="center_vertical"
		        android:text="TIPPI"
		        android:textColor="@color/black"
		        android:textSize="20sp"
		        android:typeface="serif" />

			<LinearLayout
			    android:id="@+id/mypersonalinfo_uploadPortrait_linearLayout"
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="top"
			    android:layout_weight="1"
			    android:orientation="vertical" >
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >

		            <TextView
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:gravity="center_vertical"
		                android:text="上传头像"
		                android:textColor="@color/textOrange"
		                android:textSize="17sp"
		                android:typeface="serif" />

				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
		    android:paddingBottom="5dp"
		    android:paddingTop="5dp" >

			<LinearLayout
			    android:layout_width="40dp"
			    android:layout_height="wrap_content"
			    android:orientation="horizontal" >
		        <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:src="@drawable/personinfo_gray" />
			</LinearLayout>

			<TextView
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:gravity="center_vertical"
			    android:text="个人信息"
			    android:textColor="@color/black"
			    android:textSize="17sp"
			    android:typeface="serif" />

			<LinearLayout
			    android:id="@+id/mypersonalinfo_editInfo_linearLayout"
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="top"
			    android:layout_weight="1"
			    android:orientation="vertical" >
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >

					<TextView
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_vertical"
					    android:gravity="center_vertical"
					    android:text="编辑"
					    android:textColor="@color/textOrange"
					    android:textSize="17sp"
					    android:typeface="serif" />

				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="5dp"
		    android:orientation="horizontal"
		    android:paddingBottom="5dp"
		    android:paddingTop="5dp" >

			<TextView
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:layout_marginLeft="40dp"
			    android:gravity="center_vertical"
			    android:text="姓名"
			    android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />

			<LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="top"
			    android:layout_weight="1"
			    android:orientation="vertical" >
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >

					<TextView
					    android:id="@+id/mypersonalinfo_nameInfo_textView"
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_vertical"
					    android:gravity="center_vertical"
					    android:text="TIPPI"
					    android:textColor="@color/black"
					    android:textSize="17sp"
					    android:typeface="serif" />

				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="5dp"
		    android:orientation="horizontal"
		    android:paddingBottom="5dp"
		    android:paddingTop="5dp" >

			<TextView
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:layout_marginLeft="40dp"
			    android:gravity="center_vertical"
			    android:text="性别"
			    android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />

			<LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="top"
			    android:layout_weight="1"
			    android:orientation="vertical" >
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >

					<TextView
					    android:id="@+id/mypersonalinfo_genderInfo_textView"
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_vertical"
					    android:gravity="center_vertical"
					    android:text="女"
					    android:textColor="@color/black"
					    android:textSize="17sp"
					    android:typeface="serif" />

				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="15dp"
		    android:orientation="horizontal"
		    android:paddingBottom="5dp"
		    android:paddingTop="5dp" >

			<TextView
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:layout_marginLeft="40dp"
			    android:gravity="center_vertical"
			    android:text="出生日期"
			    android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />

			<LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="top"
			    android:layout_weight="1"
			    android:orientation="vertical" >
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >

					<TextView
					    android:id="@+id/mypersonalinfo_birthdayInfo_textView"
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_vertical"
					    android:gravity="center_vertical"
					    android:text="1991-02-01"
					    android:textColor="@color/black"
					    android:textSize="17sp"
					    android:typeface="serif" />

				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:id="@+id/mypersonalinfo_changePassword_linearLayout"
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="15dp"
		    android:orientation="horizontal"
		    android:paddingBottom="5dp"
		    android:paddingTop="5dp" >

			<LinearLayout
			    android:layout_width="40dp"
			    android:layout_height="wrap_content"
			    android:orientation="horizontal" >
		        <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:src="@drawable/changepassword_gray" />
			</LinearLayout>
			<TextView
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="center_vertical"
		        android:text="修改密码"
		        android:textColor="@color/black"
		        android:textSize="17sp"
		        android:typeface="serif" />
			<LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="top"
			    android:layout_weight="1"
			    android:orientation="vertical" >
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >
					<ImageView
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_vertical"
					    android:src="@drawable/arrow_right_orange_medium" />
				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:id="@+id/mypersonalinfo_changePhoneNum_linearLayout"
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="15dp"
		    android:orientation="horizontal"
		    android:paddingBottom="5dp"
		    android:paddingTop="5dp" >
			<LinearLayout
			    android:layout_width="40dp"
			    android:layout_height="wrap_content"
			    android:orientation="horizontal" >
		        <ImageView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:src="@drawable/changephonenum_gray" />
			</LinearLayout>
			<TextView
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="center_vertical"
		        android:text="修改手机号码"
		        android:textColor="@color/black"
		        android:textSize="17sp"
		        android:typeface="serif" />
			<LinearLayout
			    android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="top"
			    android:layout_weight="1"
			    android:orientation="vertical" >
		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >
					<ImageView
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_vertical"
					    android:src="@drawable/arrow_right_orange_medium" />
				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:orientation="horizontal"
		    android:paddingBottom="5dp">

			<LinearLayout
			    android:id="@+id/mypersonalinfo_exit_linearLayout"
			    android:layout_width="match_parent"
			    android:layout_height="40dp"
			    android:layout_gravity="bottom"
			    android:layout_marginBottom="10dp"
			    android:background="@color/panelOrange"
			    android:orientation="horizontal" >
	
				<LinearLayout
				    android:layout_width="match_parent"
				    android:layout_height="wrap_content"
				    android:layout_gravity="center_vertical"
				    android:background="@color/panelOrange"
				    android:orientation="vertical" >
					<TextView
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_horizontal"
					    android:text="退出登录"
					    android:textColor="@color/white"
					    android:textSize="22sp"
					    android:textStyle="bold"
					    android:typeface="serif" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
    </LinearLayout>
</LinearLayout>

