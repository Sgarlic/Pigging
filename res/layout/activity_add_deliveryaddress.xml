<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:ignore="MergeRootFrame" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingRight="15dp" >

		<LinearLayout
		    android:id="@+id/return_logo_linearLayout"
		    android:layout_width="150dp"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >

	        <ImageView
	            android:id="@+id/return_logo_imageView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:src="@drawable/arrow_left_returntoprepage" />
        </LinearLayout>

	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_weight="1"
	        android:orientation="vertical" >

	        <LinearLayout
		        android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal">

		        <TextView
		            android:id="@+id/adddeliveryaddr_title_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:text="添加常用地址"
		            android:textColor="@color/textOrange"
		            android:textSize="20sp"
		            android:typeface="serif" />

	        </LinearLayout>
		</LinearLayout>

		<LinearLayout
		    android:id="@+id/adddeliveryaddr_complete_linearLayout"
		    android:layout_width="150dp"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >

	        <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:layout_weight="1"
		        android:orientation="vertical" >
	
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="right"
		            android:text="完成"
		            android:textColor="@color/textOrange"
		            android:textSize="19sp"
		            android:typeface="serif" />
		    </LinearLayout>
	    </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/panelOrange"
        android:orientation="horizontal" >

	</LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="20dp" >

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="20dp"
		    android:orientation="horizontal" >

			<TextView
			    android:layout_width="90dp"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:gravity="top"
			    android:maxLines="1"
			    android:text="收件人 "
			    android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />

			<EditText
			    android:id="@+id/adddeliveryaddr_input_recipientName_editText"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:background="@color/white"
			    android:ems="8"
			    android:hint="输入您的姓名"
			    android:maxLines="1"
			    android:textColor="@color/black"
			    android:textColorHint="@color/listviewDividerColor"
			    android:textSize="17sp"
			    android:typeface="serif" >

			    <requestFocus />
			</EditText>
	    </LinearLayout>

	    <LinearLayout
	        android:id="@+id/adddeliveryaddr_chooseProvince_linearLayout"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginBottom="20dp"
	        android:orientation="horizontal" >

		    <TextView
		        android:layout_width="90dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="省 "
		        android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />
		    <TextView
		        android:id="@+id/adddeliveryaddr_choosedProvince_textView"
		        android:layout_width="100dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="请选择省份"
		        android:textColor="@layout/selector_textview_of_selectedandunselected"
		        android:textSize="17sp"
		        android:typeface="serif" />

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="match_parent"
		        android:layout_gravity="top"
		        android:layout_weight="1"
		        android:orientation="vertical" >

		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >

					<ImageView
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_vertical"
					    android:layout_marginLeft="5dp"
					    android:src="@drawable/arrow_right_orange_small" />

				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		
	    <LinearLayout
	        android:id="@+id/adddeliveryaddr_chooseCity_linearLayout"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginBottom="20dp"
	        android:orientation="horizontal" >

		    <TextView
		        android:layout_width="90dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="市 "
		        android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />
		    <TextView
		        android:id="@+id/adddeliveryaddr_choosedCity_textView"
		        android:layout_width="100dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="请选择城市"
		        android:textColor="@layout/selector_textview_of_selectedandunselected"
		        android:textSize="17sp"
		        android:typeface="serif" />

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="match_parent"
		        android:layout_gravity="top"
		        android:layout_weight="1"
		        android:orientation="vertical" >

		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >

					<ImageView
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_vertical"
					    android:layout_marginLeft="5dp"
					    android:src="@drawable/arrow_right_orange_small" />

				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		
	    <LinearLayout
	        android:id="@+id/adddeliveryaddr_chooseDistrict_linearLayout"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginBottom="20dp"
	        android:orientation="horizontal" >

		    <TextView
		        android:layout_width="90dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="区 "
		        android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />
		    <TextView
		        android:id="@+id/adddeliveryaddr_choosedDistrict_textView"
		        android:layout_width="100dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="请选择地区 "
		        android:textColor="@layout/selector_textview_of_selectedandunselected"
		        android:textSize="17sp"
		        android:typeface="serif" />

		    <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="match_parent"
		        android:layout_gravity="top"
		        android:layout_weight="1"
		        android:orientation="vertical" >

		        <LinearLayout
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:layout_gravity="right"
		            android:layout_weight="1"
		            android:orientation="horizontal" >

					<ImageView
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_vertical"
					    android:layout_marginLeft="5dp"
					    android:src="@drawable/arrow_right_orange_small" />

				</LinearLayout>
		    </LinearLayout>
		</LinearLayout>

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="20dp"
		    android:orientation="horizontal" >

		    <TextView
		        android:layout_width="90dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="详细地址"
		        android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />

			<EditText
			    android:id="@+id/adddeliveryaddr_input_detailedAddr_editText"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:background="@color/white"
			    android:ems="8"
			    android:hint="请输入详细地址"
			    android:textColor="@color/black"
			    android:textColorHint="@color/listviewDividerColor"
			    android:textSize="17sp"
			    android:typeface="serif" >
			</EditText>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="20dp"
		    android:orientation="horizontal" >

		    <TextView
		        android:layout_width="90dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="邮政编码"
		        android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />

			<EditText
			    android:id="@+id/adddeliveryaddr_input_zipcode_editText"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:background="@color/white"
			    android:ems="8"
			    android:hint="请填写6位编码"
			    android:inputType="number"
			    android:maxLines="1"
			    android:maxLength="6"
			    android:textColor="@color/black"
			    android:textColorHint="@color/listviewDividerColor"
			    android:textSize="17sp"
			    android:singleLine="true"
			    android:typeface="serif" >
			</EditText>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginBottom="20dp"
		    android:orientation="horizontal" >

		    <TextView
		        android:layout_width="90dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="手机"
		        android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />

			<EditText
			    android:id="@+id/adddeliveryaddr_input_mobile_editText"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:background="@color/white"
			    android:ems="8"
			    android:hint="请填写收件人手机号"
			    android:inputType="number"
			    android:maxLines="1"
			    android:textColor="@color/black"
			    android:textColorHint="@color/listviewDividerColor"
			    android:textSize="17sp"
			    android:singleLine="true"
			    android:typeface="serif" >
			</EditText>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal" >

		    <TextView
		        android:layout_width="90dp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_vertical"
		        android:gravity="top"
		        android:text="电话"
		        android:textColor="@color/priceGray"
			    android:textSize="17sp"
			    android:typeface="serif" />

			<EditText
			    android:id="@+id/adddeliveryaddr_input_phone_editText"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:background="@color/white"
			    android:ems="8"
			    android:hint="请填写收件人电话"
			    android:maxLines="1"
			    android:textColor="@color/black"
			    android:textColorHint="@color/listviewDividerColor"
			    android:textSize="17sp"
			    android:typeface="serif"
			    android:singleLine="true">
			</EditText>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:orientation="horizontal"
		    android:paddingBottom="5dp">

			<LinearLayout
			    android:id="@+id/adddeliveryaddr_deleteAddr_linearLayout"
			    android:layout_width="match_parent"
			    android:layout_height="40dp"
			    android:layout_gravity="bottom"
			    android:layout_marginBottom="10dp"
			    android:background="@color/panelOrange"
			    android:orientation="horizontal" >
	
				<LinearLayout
				    android:layout_width="match_parent"
				    android:layout_height="wrap_content"
				    android:layout_gravity="center_vertical"
				    android:background="@color/panelOrange"
				    android:orientation="vertical" >
					<TextView
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center_horizontal"
					    android:text="删除地址"
					    android:textColor="@color/white"
					    android:textSize="22sp"
					    android:textStyle="bold"
					    android:typeface="serif" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
    </LinearLayout>
</LinearLayout>

