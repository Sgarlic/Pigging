<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:ignore="MergeRootFrame" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingRight="15dp" >

		<LinearLayout
		    android:id="@+id/return_logo_linearLayout"
		    android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal" >

	        <ImageView
	            android:id="@+id/return_logo_imageView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:src="@drawable/return_1x" />
        </LinearLayout>
	    <LinearLayout
	        android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="vertical">
	        <LinearLayout
		        android:layout_width="wrap_content"
			    android:layout_height="match_parent"
			    android:layout_gravity="center_horizontal"
			    android:orientation="horizontal">
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:text="订单支付"
		            android:textColor="@color/textOrange"
		            android:textSize="20dp"
		            android:typeface="serif" />
	        </LinearLayout>
		</LinearLayout>
		<LinearLayout
	        android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1"
		    android:orientation="horizontal">
	    </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/panelOrange"
        android:orientation="horizontal" >

	</LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="10dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="20dp" >
		<LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal" >
	        <TextView
	            android:id="@+id/orderpayment_fromto_textView"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_gravity="center_vertical"
			    android:text="上海-东京"
			    android:textColor="@color/black"
			    android:textSize="18sp"/>
	        <TextView
	            android:id="@+id/orderpayment_singleorroundway_textView"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_vertical"
	            android:layout_marginLeft="5dp"
	            android:text="单程"
	            android:textColor="@color/black"
	            android:textSize="18sp"
	            android:typeface="serif" />
			<LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:orientation="vertical" >
		        <LinearLayout
			        android:layout_width="wrap_content"
			        android:layout_height="match_parent"
			        android:layout_gravity="right"
			        android:orientation="horizontal" >
			        <TextView
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:layout_gravity="center_vertical"
			            android:layout_marginLeft="5dp"
			            android:text="订单总额："
			            android:textColor="@color/black"
			            android:textSize="18sp"
			            android:typeface="serif" />
			        <TextView
			            android:id="@+id/orderpayment_orderTotal_textView"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:layout_gravity="center_vertical"
			            android:layout_marginLeft="5dp"
			            android:text="35"
			            android:textColor="@color/black"
			            android:textSize="18sp"
			            android:typeface="serif" />
			        <TextView
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:layout_gravity="center_vertical"
			            android:layout_marginLeft="5dp"
			            android:text="元 "
			            android:textColor="@color/black"
			            android:textSize="18sp"
			            android:typeface="serif" />
			    </LinearLayout>
		    </LinearLayout>
		</LinearLayout>
		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_marginTop="10dp"
		    android:orientation="vertical" >
	        <LinearLayout
		        android:layout_width="wrap_content"
		        android:layout_height="match_parent"
		        android:layout_gravity="right"
		        android:orientation="horizontal" >
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="5dp"
		            android:text="可用波丁币抵扣："
		            android:textColor="@color/black"
		            android:textSize="18sp"
		            android:typeface="serif" />
		        <TextView
		            android:id="@+id/orderpayment_bodingDeduction_textView"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="5dp"
		            android:text="5"
		            android:textColor="@color/black"
		            android:textSize="18sp"
		            android:typeface="serif" />
		        <TextView
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_gravity="center_vertical"
		            android:layout_marginLeft="5dp"
		            android:text="元 "
		            android:textColor="@color/black"
		            android:textSize="18sp"
		            android:typeface="serif" />
		    </LinearLayout>
	    </LinearLayout>
	</LinearLayout>
	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/tabDivider"
        android:orientation="horizontal" >
	</LinearLayout>

	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:paddingLeft="15dp"
	    android:paddingRight="15dp"
	    android:paddingTop="20dp" >

		<ImageView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_gravity="top"
		    android:layout_marginTop="2dp"
		    android:src="@drawable/yanglogo_480_800" />

		<LinearLayout
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:layout_gravity="top"
		    android:layout_marginLeft="5dp"
		    android:orientation="vertical" >

		    <LinearLayout
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        android:paddingBottom="10dp" >

			    <TextView
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_gravity="top"
			        android:text="支付方式"
			        android:textColor="@color/black"
			        android:textSize="18sp"
			        android:typeface="serif" />
			</LinearLayout>

			<LinearLayout
			    android:id="@+id/orderpayment_paybyCreditCard_linearLayout"
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_gravity="top"
			    android:orientation="horizontal"
			    android:paddingBottom="10dp"
			    android:paddingTop="10dp" >

			    <TextView
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_gravity="center_vertical"
			        android:text="信用卡支付"
			        android:textColor="@color/black"
			        android:textSize="18sp"
			        android:typeface="serif" />
			    <LinearLayout
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:orientation="vertical" >
			        <LinearLayout
				        android:layout_width="wrap_content"
				        android:layout_height="match_parent"
				        android:layout_gravity="right"
				        android:orientation="horizontal" >
				        <ImageView
				            android:layout_width="wrap_content"
				            android:layout_height="wrap_content"
				            android:layout_gravity="center_vertical"
				            android:src="@drawable/check2_480_800" />
				    </LinearLayout>
			    </LinearLayout>
			</LinearLayout>

			<LinearLayout
			    android:id="@+id/orderpayment_paybySavingCard_linearLayout"
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_gravity="top"
			    android:orientation="horizontal"
			    android:paddingBottom="10dp"
			    android:paddingTop="10dp" >

			    <TextView
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_gravity="center_vertical"
			        android:text="储蓄卡支付"
			        android:textColor="@color/black"
			        android:textSize="18sp"
			        android:typeface="serif" />
			    <LinearLayout
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:orientation="vertical" >
			        <LinearLayout
				        android:layout_width="wrap_content"
				        android:layout_height="match_parent"
				        android:layout_gravity="right"
				        android:orientation="horizontal" >
				        <ImageView
				            android:layout_width="wrap_content"
				            android:layout_height="wrap_content"
				            android:layout_gravity="center_vertical"
				            android:src="@drawable/check2_480_800" />
				    </LinearLayout>
			    </LinearLayout>
			</LinearLayout>

			<LinearLayout
			    android:id="@+id/orderpayment_paybyAlipay_linearLayout"
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_gravity="top"
			    android:orientation="horizontal"
			    android:paddingBottom="10dp"
			    android:paddingTop="10dp" >

			    <TextView
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_gravity="center_vertical"
			        android:text="支付宝支付"
			        android:textColor="@color/black"
			        android:textSize="18sp"
			        android:typeface="serif" />
			    <LinearLayout
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:orientation="vertical" >
			        <LinearLayout
				        android:layout_width="wrap_content"
				        android:layout_height="match_parent"
				        android:layout_gravity="right"
				        android:orientation="horizontal" >
				        <ImageView
				            android:layout_width="wrap_content"
				            android:layout_height="wrap_content"
				            android:layout_gravity="center_vertical"
				            android:src="@drawable/check2_480_800" />
				    </LinearLayout>
			    </LinearLayout>
			</LinearLayout>

			<LinearLayout
			    android:id="@+id/orderpayment_paybyTenpay_linearLayout"
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:layout_gravity="top"
			    android:orientation="horizontal"
			    android:paddingBottom="10dp"
			    android:paddingTop="10dp" >

			    <TextView
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_gravity="center_vertical"
			        android:text="财付通支付"
			        android:textColor="@color/black"
			        android:textSize="18sp"
			        android:typeface="serif" />
			    <LinearLayout
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:orientation="vertical" >
			        <LinearLayout
				        android:layout_width="wrap_content"
				        android:layout_height="match_parent"
				        android:layout_gravity="right"
				        android:orientation="horizontal" >
				        <ImageView
				            android:layout_width="wrap_content"
				            android:layout_height="wrap_content"
				            android:layout_gravity="center_vertical"
				            android:src="@drawable/check2_480_800" />
				    </LinearLayout>
			    </LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>

